{
    "name": "httpbin - Test cache functioning",
    "api_id": "httpbin-2",
    "slug": "",
    "org_id": "default",
    
    "definition": {
        "location": "header",
        "key": "version"
    },
    "version_data": {
        "not_versioned": true,
        "versions": {
            "Default": {
                "name": "Default"
            }
        }
    },

    "use_keyless": true,

    "proxy": {
        "listen_path": "/httpbin/cache",
        "target_url": "http://echo",
        "strip_listen_path": true
    },

    "cache_options": {
        "cache_timeout": 10,
        "enable_cache": true,
        "cache_all_safe_requests": true,
        "enable_upstream_cache_control": false,
        "cache_by_headers": [],
        "cache_response_codes": [200]
    },

    "disable_rate_limit": false,
    "global_rate_limit": {
        "rate": 20,
        "per": 60
    }
}